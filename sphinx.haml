!!! 5
%head
  %meta{content:'initial-scale=1.0', name:'viewport'}
  %link{href:'https://fonts.googleapis.com/css?family=Lato:400,700|Roboto+Slab:400,700|Inconsolata:400,700',
        rel:'stylesheet',type:'text/css'}
  %link{rel:'stylesheet', href:'supplescroll.css'}
  %link{rel:'stylesheet', href:'sphinx.css'}
  - if page.title:
    %title {{ page.title }}
%body
  #table-of-contents.touchscroll
    #toc-header
      %a{href:"/"} 
        <>

  #main-text.touchscroll
    .relative_position
      - if page.title:
        {{ page.title }}
      %hr
      {{ page.content }}
      .timestamp
        <i><a href='http://github.com/boscoh/supplescroll'>supplescroll</a></i>:sphinx theme
  #figure-list.touchscroll

%script{type:'text/javascript', src:'jquery-2.0.3.js'}
%script{type:'text/javascript', src:'jquery.scrollTo.js'}
%script{type:'text/javascript', src:'supplescroll.js'}
%script{type:'text/javascript', src:'page.js'}

